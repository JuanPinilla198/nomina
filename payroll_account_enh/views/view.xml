<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_hr_payslip_form_inherit_payroll_account_enh" model="ir.ui.view">
        <field name="name">hr.payslip.view.form.inherit</field>
        <field name="model">hr.payslip</field>
        <field name="inherit_id" ref="bi_hr_payroll.view_hr_payslip_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='credit_note']" position="after">
                <field name="analytic_account_tag_id"/>
                <field name="previous_move_id" invisible='1'/>
            </xpath>
        </field>
    </record>
    <record id="view_partner_form" model="ir.ui.view">
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='industry_id']" position="after">
                <field name="ent_salud" string="Entidades de Salud"/>
            </xpath>
        </field>
    </record>
</odoo>
