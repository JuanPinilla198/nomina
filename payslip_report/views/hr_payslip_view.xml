<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_hr_payslip_form_inherit_payslip_report" model="ir.ui.view">
        <field name="name">view_hr_payslip_form_inherit_payslip_report</field>
        <field name="model">hr.payslip</field>
        <field name="inherit_id" ref="bi_hr_payroll.view_hr_payslip_form"/>
        <field name="arch" type="xml">
          <xpath expr="//field[@name='struct_id']" position="after">
                <field name="vacation_paid" string="Vacaciones Pagadas" />
            </xpath>
        </field>
    </record>
</odoo>